# PostHog keys for analytics/error tracking and build-time sourcemap uploads
NEXT_PUBLIC_POSTHOG_HOST=
NEXT_PUBLIC_POSTHOG_KEY=
POSTHOG_API_KEY=
POSTHOG_ENV_ID=

# Postgres credentials (set by Neon integration)
DATABASE_URL=

# Redis credentials (set by Upstash integration)
KV_REST_API_TOKEN=
KV_REST_API_URL=

# Inngest credentials (set by Inngest integration)
INNGEST_EVENT_KEY=
INNGEST_SIGNING_KEY=

# Vercel Cron secret for authenticating scheduled tasks
CRON_SECRET=

# Vercel Blob storage credentials (auto-configured on Vercel)
BLOB_READ_WRITE_TOKEN=

# Blob key hashing secret (required outside development)
BLOB_SIGNING_SECRET=

# Mapbox public token for react-map-gl
NEXT_PUBLIC_MAPBOX_TOKEN=

# Optional: override user agent sent with upstream requests
EXTERNAL_USER_AGENT=

# Vercel Flags SDK secret for encryption, overrides, and Flags Explorer
# Generate with: openssl rand -base64 32 | tr '+/' '-_' | tr -d '='
FLAGS_SECRET=

# Vercel Edge Config URL (set by Edge Config integration)
EDGE_CONFIG=

# Optional: Statsig Console API for Flags Explorer integration
STATSIG_CONSOLE_API_KEY=
STATSIG_PROJECT_ID=
